    /// <param name="Row">The row that is currently passing through the component</param>
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        object oCompanyList = Row.productcompanyids;
        try
        {
            if (oCompanyList != null)
            {
                string sCompanyList = oCompanyList.ToString();
                string[] sTokens = sCompanyList.Split('|');
                DateTime dtDate = Row.BusinessDate;

                for (int i = 0; i < sTokens.Length; i++)
                {
                    if (sTokens[i].Length > 0)
                    {
                        Output0Buffer.AddRow();
                        Output0Buffer.productid = Row.productid;
                        Output0Buffer.companyid = sTokens[i];
                        Output0Buffer.BusinessDate = dtDate;
                    }
                }
            }
        }
        catch (Exception ex)
